@model InfoVideo.Models.Users

@{
    ViewBag.Title = "Выдаленне";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h4>Вы ўпэўнены, што хочаце выдаліць?</h4>

<div>

    @{Html.RenderPartial("User",Model);}

