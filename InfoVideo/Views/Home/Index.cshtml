@model IEnumerable<InfoVideo.Models.Video>

@{  
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="sidebar-left">
    @{
        Html.RenderPartial("SidebarLeft", null);
    }
</div>


<div class="sidebar-right">
    @{
        Html.RenderPartial("SidebarRight", Model);
    }
</div>




@foreach (var item in Model)
{
    Html.RenderPartial("Video", item);

}






