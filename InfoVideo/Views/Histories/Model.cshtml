@using System.Web.Mvc.Ajax
@model InfoVideo.Models.History

<div class="card">

    @Html.HiddenFor(model => model.Id)
    @Html.AntiForgeryToken()

    <h2>Набыццё</h2>

    <i class="zmdi zmdi-menu menu-switch"></i>


    <div class="from-form">
        @Html.LabelFor(model => model.Users.Login):
        @Html.DisplayFor(model => model.Users.Login)
    </div>


    <div class="from-form">
        @Html.LabelFor(model => model.Edition.Box):
        @Html.DisplayFor(model => model.Edition.Box)
    </div>


    <div class="from-form">
        @Html.LabelFor(model => model.Date):
        @Html.DisplayFor(model => model.Date)
    </div>

    <div class="from-form padding-last">
        @Html.LabelFor(model => model.Price):
        @Html.DisplayFor(model => model.Price)
    </div>




    <div class="menu-list card-menu" style="display: none">
        <ul>
            <a href="@Url.Action("Details", "Histories", new {id = Model.Id})">
                <i class="zmdi zmdi-view-list-alt zmdi-hc-fw"></i>Падрабязнасці
            </a>
            <a href="@Url.Action("Edit", "Histories", new {id = Model.Id})">
                <i class="zmdi zmdi-edit zmdi-hc-fw"></i>Змяніць
            </a>

        </ul>

    </div>





</div>
